{
  "name": "puppet-qpage",
  "version": "0.1.0",
  "author": "Andrew Teixeira <teixeira@broadinstitute.org>",
  "summary": "QPage Puppet module",
  "license": "Apache-2.0",
  "source": "https://github.com/broadinstitute/puppet-qpage",
  "project_page": "https://github.com/broadinstitute/puppet-qpage",
  "issues_url": "https://github.com/broadinstitute/puppet-qpage/issues",
  "operatingsystem_support": [
    {
